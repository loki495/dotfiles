[http]
	sslVerify = false
[core]
	quotepath = false
	commitGraph = true
[gc]
	writeCommitGraph = true
[receive]
	denyCurrentBranch = updateInstead
[alias]
    conflicts = diff --name-only --diff-filter=U
[log]
	date = local
[init]
	defaultBranch = master
[commit]
    verbose = true
